<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.melgar.editarRutaController">
    <children>
        <Label text="ID" />
        <TextField fx:id="idTextField" />
        <Label text="Ingresar nueva distancia" />
        <TextField fx:id="distanciaTextField" />
        <HBox spacing="10.0">
            <Button fx:id="cancelarButton" text="Cancelar" />
            <Button fx:id="aceptarButton" text="Aceptar" onAction="#aceptarCambios" />
        </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>