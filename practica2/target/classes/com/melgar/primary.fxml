<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" 
   fx:controller="com.melgar.PrimaryController" stylesheets="@styles/vistaPrincipal.css">
   <children>
      <StackPane AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.bottomAnchor="0" AnchorPane.topAnchor="0">
         <children>
            <VBox alignment="CENTER">
               <children>
                   <Label text="InRutes"/>
                   <HBox alignment="CENTER" spacing="15.0">
                       <children>
                           <Button fx:id="cargarRutasButton" text="Cargar rutas" onAction="#cambiarCarga" />
                           <Button fx:id="generarViajesButton" text="Generar Viajes" onAction="#cambiarGeViaje" />
                           <Button fx:id="inicioViajesButton" text="Inicio Viajes" onAction="#cambiarIniciarViaje" />
                       </children>
                   </HBox>
               </children>
               <padding>
                   <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
           </VBox>
         </children>
      </StackPane>
      <ImageView fx:id="imageView" fitHeight="50" fitWidth="50" AnchorPane.leftAnchor="0" AnchorPane.topAnchor="0">
         <image>
            <Image url="@images/car.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>